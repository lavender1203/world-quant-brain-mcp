# Copy to .env and fill in your values

# Credentials
CREDENTIALS_EMAIL="your-email@example.com"
CREDENTIALS_PASSWORD="your-password"

# API Settings
# Default request timeout in seconds for BRAIN API
API_SETTINGS_TIMEOUT=30
# Optional: request concurrency controls (already have sane defaults)
BRAIN_MAX_CONCURRENCY=8
BRAIN_CREATE_SIMULATION_MAX_CONCURRENCY=6

# Forum Settings
FORUM_SETTINGS_BASE_URL="https://support.worldquantbrain.com"
# true|false
FORUM_SETTINGS_HEADLESS=true
# Timeout in seconds for page waits/selectors
FORUM_SETTINGS_TIMEOUT=15

# MCP Server (SSE)
MCP_HOST=0.0.0.0
MCP_PORT=8000
MCP_SSE_PATH=/sse
